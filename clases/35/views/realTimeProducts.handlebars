<div id="realTimeProducts" class="uk-child-width-1-4@m" uk-grid>
  {{#each products}}
    <div>
      <div class="uk-card uk-card-default">
        <div class="uk-card-media-top">
          <img alt="foto producto" />
        </div>
        <div class="uk-card-body">
          <h3 class="uk-card-title">{{ this.title }}</h3>
          <h5>USD ${{this.price}}</h5>
          {{#each this.keywords}}
            <span class="uk-badge">{{this}}</span>
          {{/each}}
          <p>{{this.description}}</p>
          <button onclick="addToCart({{this.id}})" class="uk-button uk-button-secondary uk-button-small">Agregar al carrito</button>
        </div>
      </div>
    </div>
  {{/each}}
</div>